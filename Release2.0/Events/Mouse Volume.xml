<Events>
<Event>
<ID>161</ID>
<Name>Over taskbar: Increase volume</Name>
<Enabled>1</Enabled>
<DisableAfterUse>0</DisableAfterUse>
<DeleteAfterUse>0</DeleteAfterUse>
<OneInstance>0</OneInstance>
<Trigger>
<Type>Hotkey</Type>
<Key>WheelUp</Key>
</Trigger>
<Conditions>
<Condition>
<Type>MouseOver</Type>
<MouseOverType>Window</MouseOverType>
<Negate>0</Negate>
<WindowFilter>Shell_TrayWnd</WindowFilter>
<WindowMatchType>Class</WindowMatchType>
</Condition>
</Conditions>
<Actions>
<Action>
<Type>Volume</Type>
<Action>Set volume</Action>
<Volume>+10</Volume>
</Action>
</Actions>
</Event>
<Event>
<ID>162</ID>
<Name>Over taskbar: Decrease volume</Name>
<Enabled>1</Enabled>
<DisableAfterUse>0</DisableAfterUse>
<DeleteAfterUse>0</DeleteAfterUse>
<OneInstance>0</OneInstance>
<Trigger>
<Type>Hotkey</Type>
<Key>WheelDown</Key>
</Trigger>
<Conditions>
<Condition>
<Type>MouseOver</Type>
<MouseOverType>Window</MouseOverType>
<Negate>0</Negate>
<WindowFilter>Shell_TrayWnd</WindowFilter>
<WindowMatchType>Class</WindowMatchType>
</Condition>
</Conditions>
<Actions>
<Action>
<Type>Volume</Type>
<Action>Set volume</Action>
<Volume>-10</Volume>
</Action>
</Actions>
</Event>
</Events>