<?xml version="1.0" encoding="iso-8859-1"?>
<Events>
  <Event>
    <ID>15</ID>
    <Name>Explorer: Make backspace go upwards</Name>
    <Enabled>1</Enabled>
    <DisableAfterUse>0</DisableAfterUse>
    <DeleteAfterUse>0</DeleteAfterUse>
    <OneInstance>0</OneInstance>
    <Trigger>
      <Type>Hotkey</Type>
      <Key>Backspace</Key>
    </Trigger>
    <Conditions>
      <Condition>
        <Type>If</Type>
        <Compare>CabinetWClass ExploreWClass</Compare>
        <Negate>0</Negate>
        <Operator>contains</Operator>
        <With>${Class}</With>
      </Condition>
      <Condition>
        <Type>IsRenaming</Type>
        <Negate>1</Negate>
      </Condition>
    </Conditions>
    <Actions>
      <Action>
        <Type>SetDirectory</Type>
        <Path>Upward</Path>
        <WindowFilter />
        <WindowMatchType>Active</WindowMatchType>
      </Action>
    </Actions>
  </Event>
</Events>