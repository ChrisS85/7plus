<?xml version="1.0" encoding="iso-8859-1"?>
<Events>
  <Event>
    <ID>0</ID>
    <Name>Remove always on top on maximized window</Name>
    <Enabled>1</Enabled>
    <DisableAfterUse>0</DisableAfterUse>
    <DeleteAfterUse>0</DeleteAfterUse>
    <OneInstance>0</OneInstance>
    <Trigger>
      <Type>WindowStateChange</Type>
      <Event>Window maximized</Event>
      <Filter />
      <WindowMatchType>Class</WindowMatchType>
    </Trigger>
    <Conditions>
      <Condition>
        <Type>KeyIsDown</Type>
        <Key>Alt</Key>
        <Negate>0</Negate>
        <Physical>1</Physical>
        <Toggle>0</Toggle>
      </Condition>
    </Conditions>
    <Actions>
      <Action>
        <Type>Message</Type>
        <Text>test</Text>
        <Timeout />
        <Title />
      </Action>
    </Actions>
  </Event>
  <Event>
    <ID>1</ID>
    <Name>New event</Name>
    <Enabled>0</Enabled>
    <DisableAfterUse>0</DisableAfterUse>
    <DeleteAfterUse>0</DeleteAfterUse>
    <OneInstance>0</OneInstance>
    <Trigger>
      <Type>Timer</Type>
      <Restart>1</Restart>
      <ShowProgress>1</ShowProgress>
      <Time>70000</Time>
    </Trigger>
    <Conditions />
    <Actions />
  </Event>
  <Event>
    <ID>2</ID>
    <Name>New event</Name>
    <Enabled>1</Enabled>
    <DisableAfterUse>0</DisableAfterUse>
    <DeleteAfterUse>0</DeleteAfterUse>
    <OneInstance>0</OneInstance>
    <Trigger>
      <Type>Hotkey</Type>
      <Key>F10</Key>
    </Trigger>
    <Conditions />
    <Actions>
      <Action>
        <Type>FilterList</Type>
        <Action>Keep list entries from</Action>
        <ExitOnEmptyList>1</ExitOnEmptyList>
        <Filter>7plus.exe</Filter>
        <List>${SelNNEQ}</List>
        <Operator>that are equal to</Operator>
        <Separator />
      </Action>
      <Action>
        <Type>Message</Type>
        <Text>files:${FilteredList}</Text>
        <Timeout />
        <Title />
      </Action>
    </Actions>
  </Event>
</Events>